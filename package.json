    "lint-staged": {
        "src/{main/webapp,test}/**/*.{json,js,ts,css,scss,html}": "prettier --write",
        "src/{main,test}/java/**/*.java": "bash ./linting.sh"
    },
    "scripts": {
        "postinstall": "husky install",
        "prettier:check": "prettier --check 'src/{main/webapp,test}/**/*.{json,ts,js,css,scss,html}'",
        "prettier:format": "prettier --write 'src/{main/webapp,test}/**/*.{json,ts,js,css,scss,html}'",
        "lint": "eslint . --ext .ts",
        "lint:fix": "yarn run lint -- --fix",
        "lint:doc": "tslint --config tslint-doc.json --project tsconfig.json -e 'node_modules/**'",
        "cleanup": "rimraf build/resources/main/static/",
        "clean-www": "rimraf build/resources/main/static/app/{src,build/}",
        "start": "ng serve --hmr",
        "serve": "npm run start",
        "build": "npm run webapp:prod",
        "pretest": "npm run lint",
        "testapp": "ng test --coverage --log-heap-usage -w=2",
        "test:watch": "npm run test -- --watch",
        "test": "node --expose-gc ./node_modules/.bin/jest --logHeapUsage --config ./src/test/javascript/jest.config.js --env=jsdom -w=4",
        "test:ci": "node --expose-gc ./node_modules/.bin/jest --logHeapUsage --config ./src/test/javascript/jest.config.ci.js --env=jsdom -w=1 --coverage",
        "test:coverage": "node --expose-gc ./node_modules/.bin/jest --logHeapUsage --config ./src/test/javascript/jest.config.js --env=jsdom -w=4 --coverage",
        "test:leaks": "jest --logHeapUsage --config=src/test/javascript/jest.config.js --env=jsdom --detectLeaks",
        "test:server-api-tests": "`pwd`/src/test/k6/api_tests.sh",
        "update": "yarn upgrade-interactive --latest",
        "webapp:build": "npm run clean-www && npm run webapp:build:dev",
        "webapp:build:dev": "ng build --configuration development",
        "webapp:build:prod": "ng build --configuration production",
        "webapp:dev": "ng serve",
        "webapp:dev-verbose": "ng serve --verbose",
        "webapp:prod": "npm run clean-www && npm run webapp:build:prod",
        "cypress:open": "yarn run cypress open ",
        "cypress:run": "yarn run cypress run --headless --browser chrome"
    }
}
